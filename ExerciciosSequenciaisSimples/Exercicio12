#include <stdio.h>

int main()
{
    int a, b;
    printf("Primeiro Valor: ");
    scanf("%d", &a);
    
    printf("Segundo Valor: ");
    scanf("%d", &b);
    
    if(a % b == 0 || b % a == 0)
        printf("Sao multiplos");
    else
        printf("Nao sao multiplos");
    return 0;
}
