#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#include <string.h>
int main()
{
    setlocale(LC_ALL,"Portuguese");
    /* 2) Escreva um programa em C para definir e ler um vetor A com 5 elementos construir um vetor
B de mesma dimensão que o vetor A, que deverá ser formado pelos elementos do vetor A
multiplicado por 5. Apresentar o Vetor B.int numero[5]; */
    int A[5];
    int B[5];
    int i;
    for(i=0;i<5;i++)
    {
        scanf("%d", &A[i]);
        B[i] = A[i] * 5;
    }
    for(i=0;i<5;i++)
        printf("%d;", B[i]);


    return 0;
}
