#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#include <string.h>
int main()
{
    setlocale(LC_ALL,"Portuguese");
    /* 1) Escreva um programa em C para ler um vetor com 5 elementos e apresenta-los. */
    int numero[5];
    int i;
    for(i=0;i<5;i++)
        scanf("%d", &numero[i]);
    for(i=0;i<5;i++)
        printf("%d", numero[i]);


    return 0;
}
