#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#include <string.h>
int main()
{
    setlocale(LC_ALL,"Portuguese");
    /* 3) escreva um programa em C para definir um vetor de notas com 5 elementos, ler as notas,
calcular a media das notas e escrever as notas que estão acima da média.*/

    int notas[5];
    int MediaAlta[5];
    int soma;
    int i;
    for(i=0;i<5;i++)
    {
        scanf("%d", &notas[i]);
        soma = soma + notas[i];
    }

    printf("Notas acima da Media\n");
    for(i=0;i<5;i++)
        if(notas[i] > (soma/5))
        printf("%d; ", notas[i]);


    return 0;
}
