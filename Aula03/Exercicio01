    #include <stdio.h>
    #include <stdlib.h>
    #include <string.h>

    int main()
    {
      //1) Escrever um programa em C para ler dois valores e escrever o maior
                                    deles.
       int numero1, numero2;
       printf("Primeiro Numero: ");
       scanf("%d", &numero1);
       printf("Segundo Numero: ");
       scanf("%d", &numero2);
       if (numero1 > numero2)
            printf("%d Maior que %d", numero1, numero2);
       else
            printf("%d Menor que %d", numero1, numero2);


       return 0;
    }
