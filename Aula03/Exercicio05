    #include <stdio.h>
    #include <stdlib.h>
    #include <string.h>

    int main()
    {
       /* 5) Escrever um programa em C para ler dois valores e mostrar uma
                mensagem se os valores são múltiplos ou não. */
    int num1, num2;
    printf("Primeiro Numero: ");
    scanf("%d", &num1);

    printf("Segundo Numero: ");
    scanf("%d", &num2);

    if(num1 % num2 == 0)
        printf("%d e %d Sao multiplos!", num1, num2);
    else
        printf("%d e %d Nao sao multiplos!", num1, num2);


       return 0;
    }
