    #include <stdio.h>
    #include <stdlib.h>
    #include <string.h>

    int main()
    {
        //2) Escrever um programa em C para ler trÃªs valores e escrever o maior
                                    //deles
       int numero1, numero2, numero3, maior;
       printf("Primeiro Numero: ");
       scanf("%d", &numero1);
       printf("Segundo Numero: ");
       scanf("%d", &numero2);
       printf("Terceiro Numero: ");
       scanf("%d", &numero3);

        maior = numero2;
       if (numero1 > numero2)
            maior = numero1;
       if (numero3 > maior)
            maior = numero3;

        printf("Maior Numero: %d", maior);



       return 0;
    }
