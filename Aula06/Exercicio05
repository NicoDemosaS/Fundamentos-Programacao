#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#include <string.h>
int main()
{
               /* 5) Escreva um programa em C que determine o valor de S.
            ğ‘  = 1
            1 âˆ’ 2
            4 + 3
            9 âˆ’ â‹¯ âˆ’ 10
            100 */
    int cont = 1;
    float s;
    s = 1;
    while (cont != 10){
        if(cont % 2 == 0)
            s = s + (float)cont / (cont * cont);

        if(cont % 2 == 1)
            s = s - (float)cont / (cont * cont);

        cont++;
    }
    printf("Resultado: %f", s);
    return 0;
}
