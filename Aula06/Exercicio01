#include <stdio.h>

//* Exercícios
1) escreva um programa em C para escrever a tabuada de um numero qualquer
fornecido pelo usuário *//



int main() {
    int numero, i;
    printf("Numero para Tabuada: ");
    scanf("%d", &numero);

    i = 0;
    while(i<=10){
        printf("%d x %d = %d\n", numero, i, numero*i);
        i++;
    }

}
